# read config file
. /etc/isgtools.sh

# determine month passed
if [ "$1" = "01" ] ;then
month=Jan
fi
if [ "$1" = "02" ] ;then
month=Feb
fi
if [ "$1" = "03" ] ;then
month=Mar
fi
if [ "$1" = "04" ] ;then
month=Apr
fi
if [ "$1" = "05" ] ;then
month=May
fi
if [ "$1" = "06" ] ;then
month=Jun
fi
if [ "$1" = "07" ] ;then
month=Jul
fi
if [ "$1" = "08" ] ;then
month=Aug
fi
if [ "$1" = "09" ] ;then
month=Sep
fi
if [ "$1" = "10" ] ;then
month=Oct
fi
if [ "$1" = "11" ] ;then
month=Nov
fi
if [ "$1" = "12" ] ;then
month=Dec
fi

# variable creation
today=$(date +%m%d)
day="$2"
hour="$3"
ip="$4"
date="$month.$day-$hour"
# go to the log store directory
cd $iplogpath/logs
# uncompress the appropriate log file and kill any output
tar -zxvf $iplogpath/logs/archive/$month/$day.tar.gz > /dev/null
# determine sid to search for
sid=$(cat $iplogpath/logs/$date*.txt | cut -c 1-51 | grep $ip | head -n 1 | cut -c 1-4)
# search each ubr/interface for the ip and dump it into a variable
if [ "$ubr1ip" != "0.0.0.0" ] ; then
ubr1c3=$( cat $iplogpath/logs/$date.ubr1.c3.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr1c4=$( cat $iplogpath/logs/$date.ubr1.c4.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr1c5=$( cat $iplogpath/logs/$date.ubr1.c5.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr1c6=$( cat $iplogpath/logs/$date.ubr1.c6.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
fi
if [ "$ubr2ip" != "0.0.0.0" ] ; then
ubr2c3=$( cat $iplogpath/logs/$date.ubr2.c3.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr2c4=$( cat $iplogpath/logs/$date.ubr2.c4.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr2c5=$( cat $iplogpath/logs/$date.ubr2.c5.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr2c6=$( cat $iplogpath/logs/$date.ubr2.c6.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
fi
if [ "$ubr3ip" != "0.0.0.0" ] ; then
ubr3c3=$( cat $iplogpath/logs/$date.ubr3.c3.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr3c4=$( cat $iplogpath/logs/$date.ubr3.c4.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr3c5=$( cat $iplogpath/logs/$date.ubr3.c5.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr3c6=$( cat $iplogpath/logs/$date.ubr3.c6.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
fi
if [ "$ubr4ip" != "0.0.0.0" ] ; then
ubr4c3=$( cat $iplogpath/logs/$date.ubr4.c3.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr4c4=$( cat $iplogpath/logs/$date.ubr4.c4.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr4c5=$( cat $iplogpath/logs/$date.ubr4.c5.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr4c6=$( cat $iplogpath/logs/$date.ubr4.c6.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
fi
if [ "$ubr5ip" != "0.0.0.0" ] ; then
ubr5c3=$( cat $iplogpath/logs/$date.ubr5.c3.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr5c4=$( cat $iplogpath/logs/$date.ubr5.c4.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr5c5=$( cat $iplogpath/logs/$date.ubr5.c5.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr5c6=$( cat $iplogpath/logs/$date.ubr5.c6.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )

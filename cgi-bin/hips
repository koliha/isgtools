ubr6c5=$( cat $iplogpath/logs/$date.ubr6.c5.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr6c6=$( cat $iplogpath/logs/$date.ubr6.c6.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
fi
if [ "$ubr7ip" != "0.0.0.0" ] ; then
ubr7c3=$( cat $iplogpath/logs/$date.ubr7.c3.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )
ubr7c4=$( cat $iplogpath/logs/$date.ubr7.c4.txt | cut -c 1-51 | grep $ip | tr -d [:space:] )

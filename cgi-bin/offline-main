# read config file
. /etc/isgtools.sh

# sub creation
ubr1() {
(echo "$ubruser";echo "$ubrpass";echo "terminal length 0";echo "sh cable modem | i offline";echo "exit") | nc -n $ubr1ip 23 | grep -v ÿ | grep -v User | grep -v Pass | grep -v terminal | grep -v ">" | grep -v "+"
}
